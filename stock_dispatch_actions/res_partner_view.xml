<openerp>
    <data>
        <record id="view_partner_carrier_dispatch_action_form" model="ir.ui.view">
            <field name="name">res.partner.carrier.dispatch.action.form</field>
            <field name="model">res.partner</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="delivery.view_partner_property_form" />
