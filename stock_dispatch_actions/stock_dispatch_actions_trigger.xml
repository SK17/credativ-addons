<?xml version="1.0" ?>
<openerp>
    <data noupdate="0">
        <record id="dispatch_trigger" model="sartre.trigger">
            <field name="name">Dispatch Trigger</field>
            <field name="category_id" model="sartre.category" search="[('name','=',u'Default')]"/>
            <field name="active" eval="1"/>
            <field name="model_id" model="ir.model" search="[('model','=','stock.dispatch')]"/>
            <field name="on_date">1</field>
            <field name="on_date_type_display1">other_date</field>
            <field name="on_date_type_display2_id" model="ir.model.fields" search="[('name','=','dispatch_trigger_date'),('model','=','stock.dispatch')]" />
            <field name="on_date_range">0</field>
            <field name="on_date_range_type">days</field>
            <field name="on_date_range_operand">before</field>
            <field name="interval_number">60</field>
            <field name="interval_type">minutes</field>
            <field name="run_once">1</field>
            <field name="executions_max_number">1</field>
            <field name="action_ids" eval="[]"/>
            <field name="template_language">mako</field>
        </record>
    </data>
</openerp>
