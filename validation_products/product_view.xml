<openerp>
    <data>
        <record model="ir.ui.view" id="product_product_form_view_validate">
            <field name="name">product.normal.form_validate</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view" />
            <field name="type">form</field>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='purchase_ok']" position="after">
                    <field name="validate_override" groups="validation_products.group_product_validation_manager"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
