<?xml version="1.0"?>
    <openerp>
        <data>
            <record id="prod_trans_picking_in_prod" model="workflow.transition">
                <field name="act_from" ref="mrp.prod_act_picking"/>
                <field name="act_to" ref="mrp.prod_act_in_production"/>
                <field name="signal"></field>
                <field name="condition">test_if_subcontractor_product()</field>
            </record>
            <record id="prod_trans_picking_ready" model="workflow.transition">
                <field name="act_from" ref="mrp.prod_act_picking"/>
                <field name="act_to" ref="mrp.prod_act_ready"/>
                <field name="signal"></field>
                <field name="condition">not test_if_subcontractor_product()</field>
            </record>
            <delete model="workflow.transition" id="mrp.prod_trans_picking_ready" />
        </data>
</openerp>
