<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_holiday_test" model="ir.ui.view">
            <field name="name">holiday.test.form</field>
            <field name="model">holiday.test</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Test">
                    <field name="h_date"/>
                    <separator string="" colspan="4"/>
                    <group colspan="4" col="6">
                        <button icon="gtk-cancel" special="cancel" string="Cancel"/>
                        <button icon="terp-gtk-go-back-rtl" string="Ok" name="isWorkingDay" type="object"/>
                    </group>
					<field name="isWorkingDay"/>
                </form>
            </field>
        </record>

        <record id="action_view_holiday_test" model="ir.actions.act_window">
            <field name="name">Testing Functions</field>
            <field name="res_model">holiday.test</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_holiday_test"/>
            <field name="target">new</field>
        </record>

        <menuitem icon="STOCK_INDENT" action="action_view_holiday_test"
            id="menu_action_view_holiday_test"
            parent="public_holidays.action_holiday" />
    </data>
</openerp>
