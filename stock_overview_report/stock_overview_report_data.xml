<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- Scheduler -->
        <record forcecreate="True" id="ir_cron_stock_overview_auto" model="ir.cron">
            <field name="name">Run Auto Generate Stock Overview Report</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
            <field name="model">stock.overview.report</field>
            <field name="function">autopopulate</field>
            <field name="args">()</field>
        </record>
    </data>
</openerp>